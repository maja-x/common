        <field num="7" type="DATE10" length="10"></field>
        <field num="11" type="NUMERIC" length="6"></field>
        <field num="32" type="LLVAR"></field>
        <field num="39" type="ALPHA" length="2"></field>
        <field num="70" type="NUMERIC" length="3"></field>
    </parse>
